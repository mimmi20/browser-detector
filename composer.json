{
  "name": "mimmi20/browser-detector",
  "type": "library",
  "description": "Library to detect Browsers and Devices",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/BrowserDetector",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/BrowserDetector/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "danielstjules/stringy": "^3.1",
    "localheinz/json-normalizer": "^0.9",
    "mimmi20/browser-detector-loader-interface": "~3.0.0",
    "mimmi20/browser-detector-version": "~6.0.3",
    "mimmi20/json-class": "~2.0.2",
    "mimmi20/ua-browser-type": "~7.0.2",
    "mimmi20/ua-device-type": "~7.0.2",
    "mimmi20/ua-display-size": "~1.0.3",
    "mimmi20/ua-generic-request": "~4.0.1",
    "mimmi20/ua-result": "~6.0.4",
    "peterkahl/apple-ios-build-version": "^59.13",
    "peterkahl/apple-os-x-build-version": "^43.6",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "infection/infection": "^0.11.2",
    "localheinz/composer-normalize": "^1.0.0",
    "localheinz/phpstan-rules": "^0.4 || ^0.5",
    "mikey179/vfsStream": "^1.6",
    "pepakriz/phpstan-exception-rules": "^0.3 || ^0.4",
    "phpstan/phpstan": "^0.10",
    "phpstan/phpstan-beberlei-assert": "^0.10",
    "phpstan/phpstan-deprecation-rules": "^0.10",
    "phpstan/phpstan-phpunit": "^0.10",
    "phpstan/phpstan-strict-rules": "^0.10",
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/cache": "^4.2",
    "symfony/finder": "^4.2",
    "zendframework/zend-diactoros": "^1.8 || ^2.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowserDetector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowserDetectorTest\\": "tests/DetectorTest/",
      "UserAgentsTest\\": "tests/UserAgentsTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/BrowserDetector/issues",
    "source": "https://github.com/mimmi20/BrowserDetector"
  }
}
