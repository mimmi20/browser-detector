{
  "name": "mimmi20/browser-detector",
  "type": "library",
  "description": "Library to detect Browsers and Devices",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/BrowserDetector",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/BrowserDetector/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "localheinz/json-normalizer": "^0.9.0",
    "mimmi20/browser-detector-loader-interface": "~3.0.1",
    "mimmi20/browser-detector-version": "~6.0.6",
    "mimmi20/ios-build": "~1.0.1",
    "mimmi20/json-class": "~2.0.6",
    "mimmi20/macos-build": "~1.0.1",
    "mimmi20/ua-browser-type": "~7.0.4",
    "mimmi20/ua-device-type": "~7.1.3",
    "mimmi20/ua-display-size": "~1.0.35",
    "mimmi20/ua-generic-request": "~5.0.1",
    "mimmi20/ua-normalizer": "~4.0.0",
    "mimmi20/ua-result": "~7.0.2",
    "psr/http-message": "^1.0.0",
    "psr/log": "^1.1.2",
    "psr/simple-cache": "^1.0.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16.0",
    "infection/infection": "^0.13.6",
    "localheinz/composer-normalize": "^1.3.1",
    "localheinz/phpstan-rules": "^0.13.0",
    "mikey179/vfsstream": "^1.6.8",
    "pepakriz/phpstan-exception-rules": "^0.8.3",
    "phpstan/phpstan": "^0.11.19",
    "phpstan/phpstan-beberlei-assert": "^0.11.1",
    "phpstan/phpstan-deprecation-rules": "^0.11.2",
    "phpstan/phpstan-phpunit": "^0.11.2",
    "phpstan/phpstan-strict-rules": "^0.11.1",
    "phpunit/phpunit": "^7.5.17 || ^8.4.0",
    "squizlabs/php_codesniffer": "^3.5.2",
    "symfony/cache": "^4.3.7",
    "symfony/finder": "^4.3.7",
    "zendframework/zend-diactoros": "^2.2.0"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowserDetector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowserDetectorTest\\": "tests/DetectorTest/",
      "UserAgentsTest\\": "tests/UserAgentsTest/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/BrowserDetector/issues",
    "source": "https://github.com/mimmi20/BrowserDetector"
  }
}
