{
  "name": "mimmi20/browser-detector",
  "type": "library",
  "description": "Library to detect Browsers and Devices",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/BrowserDetector",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/BrowserDetector/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "ext-json": "*",
    "mimmi20/browser-detector-loader-interface": "~3.0.13",
    "mimmi20/browser-detector-version": "~6.0.18",
    "mimmi20/ios-build": "~1.0.13",
    "mimmi20/json-class": "~2.0.17",
    "mimmi20/macos-build": "~1.0.12",
    "mimmi20/ua-browser-type": "~7.0.16",
    "mimmi20/ua-device-type": "~7.1.15",
    "mimmi20/ua-display-size": "~2.0.26",
    "mimmi20/ua-generic-request": "~5.0.13",
    "mimmi20/ua-normalizer": "~4.0.12",
    "mimmi20/ua-result": "~7.1.7",
    "psr/http-message": "~1.0.1",
    "psr/log": "~1.1.3",
    "psr/simple-cache": "~1.0.1"
  },
  "require-dev": {
    "infection/infection": "~0.13.6 || ~0.18 || ~0.20.2",
    "laminas/laminas-diactoros": "~2.4.1 || ~2.5.0",
    "mikey179/vfsstream": "~1.6.8",
    "mimmi20/coding-standard": "~1.0.9",
    "mimmi20/json-normalizer": "~1.0.11",
    "pepakriz/phpstan-exception-rules": "~0.11.5",
    "phpstan/extension-installer": "~1.1.0",
    "phpstan/phpstan": "~0.12.68",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.17",
    "phpunit/phpunit": "~7.5.20 || ~8.5.13 || ~9.5.0",
    "symfony/cache": "~4.4.18 || ~5.2.1",
    "symfony/finder": "~4.4.18 || ~5.2.1"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowserDetector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowserDetectorTest\\": "tests/DetectorTest/",
      "UserAgentsTest\\": "tests/UserAgentsTest/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/BrowserDetector/issues",
    "source": "https://github.com/mimmi20/BrowserDetector"
  }
}
