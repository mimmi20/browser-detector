{
  "name": "mimmi20/browser-detector",
  "type": "library",
  "description": "Library to detect Browsers and Devices",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/BrowserDetector",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/BrowserDetector/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "ext-json": "*",
    "mimmi20/browser-detector-loader-interface": "~3.0.6",
    "mimmi20/browser-detector-version": "~6.0.12",
    "mimmi20/ios-build": "~1.0.7",
    "mimmi20/json-class": "~2.0.11",
    "mimmi20/macos-build": "~1.0.6",
    "mimmi20/ua-browser-type": "~7.0.10",
    "mimmi20/ua-device-type": "~7.1.9",
    "mimmi20/ua-display-size": "~2.0.13",
    "mimmi20/ua-generic-request": "~5.0.7",
    "mimmi20/ua-normalizer": "~4.0.6",
    "mimmi20/ua-result": "~7.1.2",
    "psr/http-message": "^1.0.1",
    "psr/log": "^1.1.3",
    "psr/simple-cache": "^1.0.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.6.1",
    "ergebnis/phpstan-rules": "^0.15.0",
    "infection/infection": "^0.13.6 || ^0.16.3",
    "laminas/laminas-diactoros": "^2.3.1",
    "mikey179/vfsstream": "^1.6.8",
    "mimmi20/coding-standard": "~1.0.1",
    "pepakriz/phpstan-exception-rules": "^0.11.1",
    "phpstan/extension-installer": "^1.0.4",
    "phpstan/phpstan": "^0.12.32",
    "phpstan/phpstan-beberlei-assert": "^0.12.2",
    "phpstan/phpstan-deprecation-rules": "^0.12.4",
    "phpstan/phpstan-phpunit": "^0.12.11",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpunit/phpunit": "^7.5.20 || ^8.5.5 || ^9.2.2",
    "symfony/cache": "^4.4.10 || ^5.1.1",
    "symfony/finder": "^4.4.10 || ^5.1.1"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowserDetector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowserDetectorTest\\": "tests/DetectorTest/",
      "UserAgentsTest\\": "tests/UserAgentsTest/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/BrowserDetector/issues",
    "source": "https://github.com/mimmi20/BrowserDetector"
  }
}
