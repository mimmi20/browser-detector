{
  "name": "mimmi20/browser-detector",
  "description": "Library to detect Browsers and Devices",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/BrowserDetector/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/BrowserDetector",
  "support": {
    "issues": "https://github.com/mimmi20/BrowserDetector/issues",
    "source": "https://github.com/mimmi20/BrowserDetector"
  },
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "mimmi20/browser-detector-version": "^7.0.7",
    "mimmi20/ios-build": "^2.0.7",
    "mimmi20/json-class": "^4.0.0",
    "mimmi20/macos-build": "^2.0.7",
    "mimmi20/ua-browser-type": "^8.0.5",
    "mimmi20/ua-device-type": "^8.0.5",
    "mimmi20/ua-generic-request": "^6.0.6",
    "mimmi20/ua-normalizer": "^5.1.3",
    "mimmi20/ua-result": "^8.0.6",
    "psr/http-message": "^1.0.1",
    "psr/log": "^1.1.4",
    "psr/simple-cache": "^1.0.1",
    "symfony/finder": "^v5.4.0"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.4.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "BrowserDetector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowserDetectorTest\\": "tests/DetectorTest/",
      "UserAgentsTest\\": "tests/UserAgentsTest/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "bin": "echo 'bin not installed'",
    "post-install-cmd": [
      "@composer bin all install --ansi"
    ],
    "post-update-cmd": [
      "@composer bin all update --ansi"
    ]
  }
}
