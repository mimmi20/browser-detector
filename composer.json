{
    "name": "mimmi20/browser-detector",
    "description": "Library to detect Browsers and Devices",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "Browser",
        "user agent",
        "http",
        "parser",
        "user-agent"
    ],
    "authors": [
        {
            "name": "Thomas MÃ¼ller",
            "homepage": "https://github.com/mimmi20",
            "role": "Developer"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/mimmi20/BrowserDetector/graphs/contributors"
        }
    ],
    "homepage": "https://github.com/mimmi20/BrowserDetector",
    "support": {
        "issues": "https://github.com/mimmi20/BrowserDetector/issues",
        "source": "https://github.com/mimmi20/BrowserDetector"
    },
    "require": {
        "php": "^8.1.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "laminas/laminas-diactoros": "^3.2.0",
        "mimmi20/browser-detector-version": "^9.0.2",
        "mimmi20/ios-build": "^3.0.3",
        "mimmi20/macos-build": "^3.0.2",
        "mimmi20/ua-browser-type": "^9.0.2",
        "mimmi20/ua-device-type": "^9.0.2",
        "mimmi20/ua-normalizer": "^6.0.2",
        "psr/http-message": "^2.0",
        "psr/log": "^3.0.0",
        "psr/simple-cache": "^3.0.0"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-simplexml": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "infection/infection": "^0.27.2",
        "mikey179/vfsstream": "^1.6.11",
        "mimmi20/coding-standard": "^5.1.17",
        "nikic/php-parser": "^v4.17.1",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10.35",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-phpunit": "^1.3.14",
        "phpunit/phpunit": "^10.4.1",
        "rector/rector": "^0.18.4"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "BrowserDetector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrowserDetectorTest\\": "tests/DetectorTest/",
            "UserAgentsTest\\": "tests/UserAgentsTest/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "lock": false,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "infection/extension-installer": true
        }
    }
}
