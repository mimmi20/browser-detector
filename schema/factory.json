{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "rules": {
      "anyOf": [
        {
          "type": "object",
          "minItems": 1
        },
        {
          "type": "array"
        }
      ],
      "uniqueItems": true,
      "items": {
        "type": "string"
      }
    },
    "generic": {
      "type": "string"
    }
  },
  "required": [
    "rules",
    "generic"
  ]
}
